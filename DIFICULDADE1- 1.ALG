Algoritmo "Dificuldade1- 1"

Var
   veto: Vetor[0..4] De Inteiro
   i: Inteiro
   
Inicio
   // Leitura dos 5 números
   Para i <- 0 Até 4 Faça
      Escreva(" ")
      Leia(veto[i])
   FimPara

   // Exibição do índice e valor
   Para i <- 0 Até 4 Faça
      Escreva("Índice ", i, ": ", veto[i])
      Escreval("")
   FimPara
   
FimAlgoritmo